      webdav_config:
        username: admin
        password: "<%= common_password %>"
        private_endpoint: https://blobstore.service.cf.internal:4443
        public_endpoint: https://blobstore.<%= root_domain %>
        secret: "<%= common_password %>"
    resource_pool:
